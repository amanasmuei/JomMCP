# JomMCP Web UI Environment Variables

# API Configuration
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000
NEXT_PUBLIC_WS_BASE_URL=ws://localhost:8000

# JWT Configuration
NEXT_PUBLIC_JWT_SECRET=your-secret-key-here-change-in-production

# Application Configuration
NEXT_PUBLIC_APP_NAME=JomMCP Platform
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_APP_DESCRIPTION=API to MCP Server Automation Platform

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_SENTRY=false

# Development Configuration
NODE_ENV=development
NEXT_TELEMETRY_DISABLED=1
